<h1>New listing</h1>  
  
  <%= form_with(model: @listing) do |form|%>
    <div>
      <%= form.label :title %>
      <%= form.text_field :title %>
    </div>
    <div>
      <%= form.label :description %>
      <%= form.text_area :description, rows: 3 %>
    </div>
    <div>
      <%= form.label :location %>
      <%= form.text_field :location %>
    </div>
    <div>
      <%= form.label :price %>
      <%= form.text_field :price %>
    </div>

    <%= form.button %>
  <%end%>
